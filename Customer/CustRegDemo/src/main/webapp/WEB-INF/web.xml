<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

<servlet>
	<servlet-name>visitprofServlet</servlet-name>
	<servlet-class>com.customer.visitprofServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>visitprofServlet</servlet-name>
	<url-pattern>/visitprofile</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>customerInsert</servlet-name>
	<servlet-class>com.customer.customerInsert</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>customerInsert</servlet-name>
	<url-pattern>/Insert</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>UpdateCustomer</servlet-name>
	<servlet-class>com.customer.UpdateCustomerServlet</servlet-class>
	
</servlet>
<servlet-mapping>

<servlet-name>UpdateCustomer</servlet-name>
<url-pattern>/update</url-pattern>

</servlet-mapping>

<servlet>
<servlet-name>DeleteCustomer</servlet-name>
<servlet-class>com.customer.deletecustomerservlet</servlet-class>
</servlet>
<servlet-mapping>

<servlet-name>DeleteCustomer</servlet-name>
<url-pattern>/delete</url-pattern>

</servlet-mapping>



</web-app>
